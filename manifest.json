{
    "manifest_version": 3,
    "name": "devdiv",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "permissions": [
        "activeTab",
        "tabs"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "logo_3.png"
        }
    },
    "icons": {
        "16": "logo_3.png"
    }
}
